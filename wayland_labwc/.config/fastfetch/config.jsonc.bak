{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
  "logo":{
  	"source" : "arch",
    "type": "small",
  },

  "general": {
    "multithreading": true
  },
  "display": {
    "separator": "➜   "
  },
  "modules": [
   
    
    {
      "type": "custom",
      "format": "┌───────────────────────────────────────────┐", // `\u001b` is `\033`, or `\e`
      "outputColor": "underline_"
    },
    {
      "type": "title",
      "format": "             {6}{7}{8}",
      "outputColor": "magenta"
      
    },
    {
      "key": "     OS           ",
      "keyColor": "yellow",
      "type": "os"
    },
    {
      "key": "    󰌢 Machine      ",
      "keyColor": "green",
      "type": "host"
    },
    {
      "key": "     Kernel       ",
      "keyColor": "magenta",
      "type": "kernel"
    },
    {
      "key": "      Packages    ",
      "keyColor": "cyan",
      "type": "packages"
    },
    {
      "key": "     WM           ",
      "keyColor": "yellow",
      "type": "wm"
    },
    {
      "key": "     Theme        ",
      "keyColor": "green",
      "type": "wmtheme"
    },
    {
      "key": "     Shell        ",
      "keyColor": "magenta",
      "type": "shell"
    },
    {
      "key": "    󰻠 CPU          ",
      "keyColor": "cyan",
      "type": "cpu"
    },
    {
      "key":  "    🖴 Disk         ",
      "keyColor": "yellow",
      "type": "disk"
    },
    {
      "key":  "    🖴  Swap        ",
      "keyColor": "green",
      "type": "swap"
    },
    {
      "key": "    󰑭 Memory       ",
      "keyColor": "magenta",
      "type": "memory"
    },
    {
      "key": "     UpTime       ",
      "keyColor": "yellow",
      "type": "uptime"
    },
    {
      "type": "custom",
      "format": "└───────────────────────────────────────────┘", // `\u001b` is `\033`, or `\e`
     "outputColor": "underline_"
    },
    {
      "type": "colors",
      "paddingLeft": 20,
      "symbol": "circle",
      "block": {
        "width": 20
      }
    }
  ]
}
